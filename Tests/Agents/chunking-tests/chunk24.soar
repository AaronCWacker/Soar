# Negative conjunctive conditions

sp {apply24
    (state <s> ^operator <o>
               ^superstate <ss>)
    (<ss> ^test-item { > <y> <x> }
          ^test-item { <> <x> <y> }
          ^test-current 1)
         -{(<ss> ^test-item { > <x> > <y> <max> }
                 ^foo <max>
                 ^bar <x>)}
-->
    (<ss> ^highest <x> ^second-highest <y> ^unbound <max> ^test-done 1)
}


