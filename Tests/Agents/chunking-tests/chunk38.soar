learn --on
watch --learn 2

sp {init
   (state <s> ^superstate nil)
-->
   (<s> ^word object)

}

sp {propose*stop
   (state <s> ^superstate.superstate.superstate nil)
-->
    (write (crlf) |Too deep.|)
    (interrupt)
}

sp {propose
    (state <s> ^superstate <ss>)
    (<ss> ^superstate nil
          ^word <w>)
-->
    (<s> ^operator <o> + =)
    (<o> ^word <w>)
}

sp {apply*loose*constraint
    (state <s> ^superstate <ss>
               ^operator <o>)
    (<o> ^word <> not-object)
    -->
    (<s> ^sub-result1 1 +)
}

sp {apply*literalization
    (state <s> ^superstate <ss>
               ^operator <o>)
    (<o> ^word object)
    -->
    (<s> ^sub-result2 1 +)
}

sp {make-chunk1
    (state <s> ^superstate <ss>
               ^operator <o>
               ^sub-result1 <r>)
    (<o> ^word <object>)
    -->
    (<ss> ^result <object> +)
}

sp {make-chunk2
    (state <s> ^superstate <ss>
               ^operator <o>
               ^sub-result2 <r>)
    (<o> ^word <object>)
    -->
    (<ss> ^result2 <object> +)
}
