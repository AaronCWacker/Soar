# Possible o_id propagation problem.  

learn --on
watch --learn 2

sp {init
   (state <s> ^superstate nil)
-->
   (<s> ^word object ^word2 not-object)
}

sp {propose*stop
   (state <s> ^superstate.superstate.superstate nil)
-->
    (write (crlf) |Too deep.|)
    (interrupt)
}

sp {propose
    (state <s> ^superstate <ss>)
    (<ss> ^superstate nil
          ^word <w>)
-->
    (<s> ^operator <o> + =)
    (<o> ^word1 <w>
         ^word2 <w>)
}

sp {apply*literalization
    (state <s> ^superstate <ss>
               ^operator <o>)
    (<o> ^word2 object)
    -->
    (<s> ^sub-result-lit ungrounded +)
}

sp {apply*constraints
    (state <s> ^superstate <ss>
               ^operator <o>
               ^sub-result-lit)
    (<o> ^word1 { <> <w2> <w> }
         ^word2 <w2>)

    -->
    (<s> ^sub-result1 ungrounded +)
}

sp {apply*constraints*no-lit
    (state <s> ^superstate <ss>
               ^operator <o>)
    (<o> ^word1 { <> <w2> <w> }
         ^word2 <w2>)

    -->
    (<s> ^sub-result2 ungrounded +)
}

sp {start-chunking
    (state <s> ^superstate <ss>
               ^operator <o>
               ^sub-result-lit <r> 
               ^sub-result1 <r>)
    (<ss> ^word2 <not-object>
          ^word <object2>)
    -->
    (<ss> ^result1a <object> +
          ^result1b <not-object> +
          ^result1c <ungrounded> +)
}

sp {start-chunking-partial-lit
    (state <s> ^superstate <ss>
               ^operator <o>
               ^sub-result-lit <r> 
               ^sub-result2 <r>)
    (<ss> ^word2 <not-object>
          ^word <object2>)
    -->
    (<ss> ^result2a <object> +
          ^result2b <not-object> +
          ^result2c <ungrounded> +)
}

