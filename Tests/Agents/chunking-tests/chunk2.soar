sp {apply2
    (state <s> ^name substate
               ^operator.name test
               ^superstate <ss>)
    (<ss> ^test-item { <test> << 8 9 >> > <min>}
          ^test-item <min>
          ^test-current 1)
-->
    (<ss> ^result2a <test>
          ^result2b <unbound>
          ^test-done 1)
        (exec succeeded)
    (halt)
}

