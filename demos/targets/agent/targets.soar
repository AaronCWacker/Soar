#source extract.soar
source models.soar
#source epmem.soar
source goal.soar
source control.soar
source search.soar
source temp.soar

waitsnc -e

sp {init
   (state <s> ^superstate nil)
-->
   (<s> ^time 0
        ^task-state t
        ^auto-control f
        ^cant-change <cc>)}

sp {goal
   (state <s> ^superstate nil)
-->
   (<s> ^goal-state <gs>)
   (<gs> ^positive <p> ^negative <n>)
   (<p> ^intersect <i>)
   (<i> ^a c1 ^b c2)}

sp {model-ready
   (state <s> ^svs.command.assign-model.status success)
-->
   (<s> ^model-ready t)}
