sp {init
   (state <s> ^superstate nil)
-->
   (<s> ^time 0 ^task-state t)}

sp {define-output
   (state <s> ^superstate nil
              ^svs.command <c>)
-->
   (<c> ^output <out>)
   (<out> ^dx <l> ^dy <r>)
   (<l> ^min -1.0 ^max 1.0 ^default 0.0 ^increment 0.2)
   (<r> ^min -1.0 ^max 1.0 ^default 0.0 ^increment 0.2)}
