source plan-common.soar
source search.soar
source epmem-decide.soar
source models.soar
source control.soar

epmem --set trigger none

sp {goal
   (state <s> ^superstate nil)
-->
   (<s> ^goal-state <gs>)
   (<gs> ^positive <p> ^negative <n>)
   (<p> ^intersect <i>)
   (<i> ^a c1 ^b c2)}

sp {elab*negation
   (state <s> ^superstate nil)
-->
   (<s> ^negations <n>)
   (<n> ^positive negative
        ^negative positive)}

